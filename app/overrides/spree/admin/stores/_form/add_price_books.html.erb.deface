<!-- insert_bottom 'div[data-hook="admin_store_form_fields_right"]' -->

<div data-hook="price_books">
  <%= f.field_container :price_books, class: ['form-group'] do %>
    <%= f.label :pprice_books, Spree::PriceBook.model_name.human(count: 2) %><br />
    <%= f.collection_check_boxes :price_book_ids, Spree::PriceBook.all, :id, :name do |b| %>
      <%= b.label(class: "sub") { b.check_box + ' ' + b.text } %>
      <br/>
    <% end %>
  <% end %>
</div>